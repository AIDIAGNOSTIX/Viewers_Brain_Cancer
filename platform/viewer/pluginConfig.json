{
  "directories": {
    "src": {
      "relativeTo": "OHIF",
      "directory": ".."
    },
    "config-point": {
      "comment": "This example shows how to add a directory location, with git information",
      "relativeTo": "src",
      "directory": "config-point",
      "git": "https://github.com/OHIF/config-point.git"
    }
  },
  "extensions": [
    {
      "packageName": "@ohif/extension-cornerstone",
      "version": "3.0.0"
    },
    {
      "packageName": "@ohif/extension-tmtv",
      "version": "3.0.0"
    },
    {
      "packageName": "@ohif/extension-measurement-tracking",
      "version": "3.0.0"
    },
    {
      "packageName": "@ohif/extension-cornerstone-dicom-sr",
      "version": "3.0.0"
    },
    {
      "packageName": "@ohif/extension-default",
      "version": "3.0.0"
    },
    {
      "packageName": "@ohif/extension-dicom-pdf",
      "version": "3.0.1"
    },
    {
      "packageName": "@ohif/extension-dicom-video",
      "version": "3.0.1"
    }
  ],
  "modes": [
    {
      "packageName": "@ohif/mode-longitudinal",
      "version": "3.0.0"
    }
  ],
  "packages": [
    "@ohif/core",
    "@ohif/ui",
    "dcmjs",
    "react",
    {
      "comment": "This is a UMD dependency example, available only if checked out and built in the config-point directory above.",
      "packageName": "config-point",
      "relativeTo": "config-point",
      "directory": "."
    }
  ]
}